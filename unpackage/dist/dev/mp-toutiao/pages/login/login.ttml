<view class="page page-fill"><scroll-view class="{{(('page')+' '+(modalName!=null?'show':''))}}" scroll-y="{{modalName==null}}"><view class="header"><block tt:if="{{userIsLogin}}"><view><image class="face" src="../../static/icos/108x108.png"></image></view></block><block tt:else><view><image class="face" src="../../static/233.png"></image></view></block><block tt:if="{{userIsLogin}}"><view class="info-wapper"><view class="nickname">{{userInfo.fullname}}</view><view class="nav-info">{{userInfo.rolename}}</view></view></block><block tt:else><view><view class="nickname regist-login"><navigator url="../registLogin/registLogin">注册/登录</navigator></view></view></block><block tt:if="{{userIsLogin}}"><view class="set-wapper"><navigator url="../meInfo/meInfo"><image class="settings" src="../../static/icos/settings.png"></image></navigator></view></block></view><block tt:if="{{userIsLogin}}"><view><view class="cu-bar bg-white solid-bottom margin-top"><view data-event-opts="{{[['tap',[['getAllUser',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-title text-orange "></text>点击查看成员列表</view><view class="action"><button class="cu-btn bg-green shadow" data-target="DialogModal1" data-event-opts="{{[['tap',[['showModal',['$event']]]]]}}" bindtap="__e">添加用户</button></view></view><view class="{{(('cu-modal')+' '+(modalName=='DialogModal1'?'show':''))}}"><view class="cu-dialog"><form data-event-opts="{{[['submit',[['add_formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="cu-bar bg-white justify-end"><view class="content">添加新用户</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl"><view class="add-new-user"><label class="words-lbl">用户Id:</label><input name="userId" type="text" value="" placeholder="请输入用户Id" placeholder-class="graywords"/></view><view class="add-new-user"><label class="words-lbl">登录Id:</label><input name="loginId" type="text" value="" placeholder="请输入登录Id" placeholder-class="graywords"/></view><view class="add-new-user"><label class="words-lbl">全名:</label><input name="fullName" type="text" value="" placeholder="请输入全名" placeholder-class="graywords"/></view><view class="add-new-user"><label class="words-lbl">密码:</label><input name="pwd" type="text" value="" placeholder="请输入密码" placeholder-class="graywords"/></view><view class="add-new-user"><label class="words-lbl">状态:</label><input name="state" type="number" value="" placeholder="请输入状态" placeholder-class="graywords"/></view><view class="add-new-user"><label class="words-lbl">描述:</label><input name="describe" type="text" value="" placeholder="请输入描述" placeholder-class="graywords"/></view></view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="cu-btn line-green text-green" bindtap="__e">取消</button><button class="cu-btn bg-green margin-left" form-type="submit">确定</button></view></view></form></view></view><view class="cu-list menu-avatar"><block tt:for="{{allUser}}" tt:for-item="item" tt:for-index="index" tt:key="index"><view class="{{(('cu-item')+' '+(modalName=='move-box-'+index?'move-cur':''))}}" data-target="{{'move-box-'+index}}" data-event-opts="{{[['touchstart',[['ListTouchStart',['$event']]]],['touchmove',[['ListTouchMove',['$event']]]],['touchend',[['ListTouchEnd',['$event']]]]]}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e"><view class="cu-avatar round lg" style="{{'background-image:'+('url(https://ossweb-img.qq.com/images/lol/web201310/skin/big2100'+(index+2)+'.jpg)')+';'}}"></view><view class="content"><view class="text-grey">{{"用户名："+item.fullname}}</view><view class="text-gray text-sm"><text class="cuIcon-infofill text-red  margin-right-xs"></text>{{'密码：'+item.pwd+''}}</view></view><view class="action"><view class="text-grey text-xs">state</view><view class="cu-tag round bg-grey sm">{{item.state}}</view></view><view class="move"><view class="bg-grey" data-target="DialogModal2" data-thisitem="{{item}}" data-event-opts="{{[['tap',[['data2modal',['$event']]]],['tap',[['showModal',['$event']]]]]}}" bindtap="__e">修改</view><view class="bg-red" data-userid="{{item.userid}}" data-event-opts="{{[['tap',[['userDelete',['$event']]]]]}}" bindtap="__e">删除</view></view></view></block></view><view class="{{(('cu-modal')+' '+(modalName=='DialogModal2'?'show':''))}}"><view class="cu-dialog"><form data-event-opts="{{[['submit',[['update_formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="cu-bar bg-white justify-end"><view class="content">修改</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl"><view class="add-new-user"><label class="words-lbl">用户Id:</label><input name="userId" type="text" value="{{thisItem.userid}}"/></view><view class="add-new-user"><label class="words-lbl">登录Id:</label><input name="loginId" type="text" value="{{thisItem.loginid}}"/></view><view class="add-new-user"><label class="words-lbl">全名:</label><input name="fullName" type="text" value="{{thisItem.fullname}}"/></view><view class="add-new-user"><label class="words-lbl">密码:</label><input name="pwd" type="text" value="{{thisItem.pwd}}"/></view><view class="add-new-user"><label class="words-lbl">状态:</label><input name="state" type="number" value="{{thisItem.state}}"/></view><view class="add-new-user"><label class="words-lbl">描述:</label><input name="describe" type="text" value="{{thisItem.describe}}"/></view></view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="cu-btn line-green text-green" bindtap="__e">取消</button><button class="cu-btn bg-green margin-left" form-type="submit">确定</button></view></view></form></view></view></view></block></scroll-view></view>