.page-fill {
	width: 100%;
	height: 100%;
}

/* 头部个人信息 start */

/* 头部-外壳： */
.header {
	/* 导航栏-小程序 */

	padding: 0rpx 30rpx 40rpx 30rpx;

	/* 导航栏-非小程序 */



	background-color: #ffd655;
	/* 导航栏背景图片 */
	background: url(http://122.152.205.72:88/group1/M00/00/03/CpoxxFwu_1OAYFAIAAAzO_Q1tPg693.png) repeat;

	/* 导航栏-flex布局：横向、从左到右  */
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
}

/* 头像 */
.face {
	width: 120rpx;
	height: 120rpx;
	/* 圆形 */
	border-radius: 50%;
	/* 防止头像被挤压:控制元素缩小比例-0不缩放 */
	-webkit-flex-shrink: 0;
	        flex-shrink: 0;
}

/* 用户名称ID-外壳 */
.info-wapper {
	width: 80%;
	margin-left: 40rpx;
	/* flex布局-纵向 */
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}

/* 名称 */
.nickname {
	color: #6a5018;
	font-size: 18px;
	font-weight: bold;
}

/* ID */
.nav-info {
	color: #a38e62;
	font-size: 14px;
	margin-top: 10rpx;
}

/* 注册/登录 */
.regist-login {
	margin-left: 60rpx;
	margin-top: 30rpx;
}

/* 设置-外壳: */
.set-wapper {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: end;
	-webkit-justify-content: flex-end;
	        justify-content: flex-end;
	width: 15%;
}

/* 设置: */
.settings {
	width: 40rpx;
	height: 40rpx;
}

/* 头部个人信息 end */

/* 中间的 NEXT 大LOGO start */
.body {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.big-next {
	font-size: 160rpx;
	color: #F7F7F7;
}

